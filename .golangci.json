{
	"linters": {
		"disable-all": true,
		"enable": [
			"govet",
			"revive",
			"goimports",
			"misspell",
			"ineffassign",
			"gofmt"
		]
	},
	"run": {
		"timeout": "5m"
	},
	"issues": {
		"skip-files": [
			"/zz_generated_"
		],
		"exclude-rules": [
			{
				"path": "_test.go",
				"linters": "revive"
			}
		]
	}
}